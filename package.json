{
  "name": "transaction-checker",
  "version": "1.0.0",
  "description": "checks if tokens exists in transactions",
  "main": "src/app.ts",
  "author": "SAM CLI",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "unit": "jest",
    "lint": "eslint '**/*.ts' --quiet --fix",
    "compile": "tsc --noEmit",
    "test": "npm run compile && npm run unit",
    "run:debug": "npm run compile && dotenv tsx tests/event/invoke.ts",
    "gql:compile": "cd ./src/arweave && graphql-codegen && cd ../..",
    "gql:compile:watch": "cd ./src/arweave && graphql-codegen -w && cd ../.."
  },
  "dependencies": {
    "@apollo/client": "~3.11.8",
    "@aws-lambda-powertools/logger": "~2.9.0",
    "axios": "~1.7.7",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.1",
    "cross-fetch": "~4.0.0",
    "dotenv": "~16.4.5",
    "esbuild": "~0.23.1",
    "graphql": "~16.9.0"
  },
  "devDependencies": {
    "@eslint/js": "~9.12.0",
    "@graphql-codegen/cli": "~5.0.3",
    "@graphql-codegen/client-preset": "~4.4.0",
    "@graphql-typed-document-node/core": "~3.2.0",
    "@jest/globals": "~29.7.0",
    "@parcel/watcher": "~2.4.1",
    "@redstone-finance/sdk": "~0.6.2",
    "@types/aws-lambda": "~8.10.145",
    "@types/eslint__js": "~8.42.3",
    "@types/jest": "~29.5.13",
    "@types/node": "~22.7.5",
    "@typescript-eslint/eslint-plugin": "~8.8.1",
    "@typescript-eslint/parser": "~8.8.1",
    "dotenv-cli": "~7.4.2",
    "eslint": "~9.12.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-prettier": "~5.2.1",
    "jest": "~29.7.0",
    "prettier": "~3.3.3",
    "ts-jest": "~29.2.5",
    "ts-node": "~10.9.2",
    "tsx": "~4.19.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "~8.8.1"
  },
  "graphql": {
    "schema": "./src/arweave/schema.graphql",
    "documents": "**/*.{graphql,js,ts,mts,tsx,jsx}"
  }
}
